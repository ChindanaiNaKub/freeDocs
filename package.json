{
  "name": "freedocs-viewer",
  "version": "1.0.0",
  "description": "Diff-aware Google Docs assignment viewer via Internet Archive",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "echo 'No build step required for Express app'",
    "build:gh-pages": "cp -r public/* . && echo 'Static files copied for GitHub Pages'",
    "deploy": "npm run build:gh-pages && gh-pages -d .",
    "predeploy": "npm run build:gh-pages",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@playwright/test": "^1.55.0",
    "axios": "^1.5.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "rate-limiter-flexible": "^2.4.2"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "google-docs",
    "Internet Archive",
    "diff",
    "assignment",
    "viewer"
  ],
  "author": "",
  "license": "MIT"
}
